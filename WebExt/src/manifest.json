{
  "manifest_version": 3,
  "name": "WebExt-NativeMessage",
  "version": "0.0.1",
  "description": "Extension to connect to native app",
  "key": "",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  }
}